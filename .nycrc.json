{
	"include": ["dist/src/**/*.js"],
	"exclude-after-remap": false,
	"all": true,
	"check-coverage": true,
	"source-map": true,
	"sourceMap": true,
	"instrument": false,
	"produce-source-map": true,
	"watermarks": {
		"statements": [50, 80],
		"functions": [50, 80],
		"branches": [50, 80],
		"lines": [50, 80]
	}
}
